<?xml version="1.0" encoding="UTF-8"?>
<krpano version="1.19-pr16">

    
  <!-- Spot Style - Вход в парадную -->


  <style name="IconВходвпарадную"
         url="%FIRSTXML%/spots/_1090108810771083108210722_user_defaultpoint0.png"
         visible="false"
         edge="center"
         onhover="onhoverIconВходвпарадную"
         onover="onoverIconВходвпарадную"
         onout="onoutIconВходвпарадную"


         />
  <action name="onhoverIconВходвпарадную">
    showtooltip('hotspot');
  </action>
  <action name="onoverIconВходвпарадную">
tween(scale, 1.2);


  </action>
  <action name="onoutIconВходвпарадную">
    hideTooltip();
    tween(scale, 1);


  </action>



    
